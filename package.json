  },
  "keywords": [
    "mern",
    "mongodb",
    "react",
    "express",
    "node"
  ],
  "author": "AbhimanyuD",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^3.6.1",
    "express": "^4.16.4",
    "forever": "^3.0.2",
    "mongoose": "^5.8.3",
    "path": "^0.12.7"
  }
}
